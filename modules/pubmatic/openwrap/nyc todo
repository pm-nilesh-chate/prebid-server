IP = util.GetIP(httpReq)
.Device.Ua = httpReq.UserAgent()

lang := (*rtbReq.Device.Language)[0:2]
if util.ValidCode(lang) {
	*rtbReq.Device.Language = lang

	cookie, err := httpReq.Cookie(constant.KADUSERCOOKIE)
	*rtbReq.User.CustomData = cookie.Value

	// from interface to object
	rtbReq.User.Ext = openrtb_ext.ExtUser{}
	rtbReq.Regs.Ext = ortb_ext.ExtRegs{}
	rtbReq.Ext = ortb_ext.ExtRequest{}
	For each Imp -> imp.Ext = ortb_ext.ImpExtension{}
	for all wlang if util.ValidCode(lang) wlang=  wlang[i][0:2]
	for all content.Language if util.ValidCode(content.Language) content.Language=  content.Language[i][0:2]
	